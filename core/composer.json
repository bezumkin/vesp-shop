{
  "name": "vesp/vesp",
  "license": "MIT",
  "version": "1.0.0",
  "description": "Vesp Framework",
  "authors": [
    {
      "name": "Vasily Naumkin",
      "email": "bezumkin@yandex.ru"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "require": {
    "ext-fileinfo": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "akrabat/ip-address-middleware": "^2.1",
    "bacon/bacon-qr-code": "^2.0",
    "fenom/fenom": "^3.0",
    "guzzlehttp/guzzle": "^7.8",
    "k-samuel/faceted-search": "^3.1",
    "pelago/emogrifier": "^7.0",
    "peppeocchi/php-cron-scheduler": "^4.0",
    "phpmailer/phpmailer": "^6.8",
    "ramsey/uuid": "^4.7",
    "vesp/core": "^2.3.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.19",
    "itsgoingd/clockwork": "^4.1"
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "db:create": "phinx create",
    "db:migrate": "phinx migrate",
    "db:rollback": "phinx rollback",
    "db:rollback-all": "phinx rollback -t0",
    "db:seed": "phinx seed:run",
    "db:seed-one": "phinx seed:run -s"
  }
}
